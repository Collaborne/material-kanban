      "react": "^17.0.0 || ^18.0.0",
      "react-dom": "^17.0.0 || ^18.0.0"
    },
    "eslint-config-standard-react": {
      "eslint": "^8"
    }
  },
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ]
