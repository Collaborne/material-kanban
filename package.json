{
  "name": "@collaborne/material-kanban",
  "version": "0.8.6",
  "description": "Material styled Kanban board",
  "author": "Collaborne",
  "license": "MIT",
  "repository": "Collaborne/material-kanban",
  "main": "dist/index.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc --noEmit false",
    "test": "run-s test:lint test:build test:unit",
    "test:build": "tsc --noEmit -p tsconfig.test.json",
    "test:lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test:unit": "cross-env CI=1 SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom --passWithNoTests",
    "test:watch": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom",
    "predeploy": "run-s storybook:build",
    "deploy": "gh-pages -d storybook-static -b deploy/gh-pages",
    "storybook": "start-storybook -p 9009",
    "storybook:build": "build-storybook --quiet"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-actions": "^6.1.0-beta.1",
    "@storybook/addon-backgrounds": "^6.1.0-beta.1",
    "@storybook/addon-docs": "^6.1.0-beta.1",
    "@storybook/addon-links": "^6.1.0-beta.1",
    "@storybook/addons": "^6.1.0-beta.1",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.1.0-beta.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.0.2",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-scripts": "^4.0.1",
    "typescript": "4.2.2"
  },
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "dependencies": {
    "@types/react-beautiful-dnd": "^13.0.0",
    "react-beautiful-dnd": "^13.0.0"
  }
}
